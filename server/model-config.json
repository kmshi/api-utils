{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Helper": {
    "dataSource": "transient",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "captureHTMLScreen": true,
          "copyRemoteFile": true,
          "recognize": true,
          "uploadToken": true
        }
      }
    }
  }
}
